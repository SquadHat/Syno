Emot:NewKeybind("Distraction", "Distraction", Enum.KeyCode.Zero, function()
Rep3:InvokeServer("Emotes", "Distraction", "Emote1")
Rep4:FireServer("1")
Rep3:InvokeServer("Emotes", "Catdown", "Emote1") 
end)

-- UI --

Key:NewKeybind("UI", "Toggle", Enum.KeyCode.K, function()
	Library:ToggleUI()
end)
