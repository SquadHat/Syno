-- yes no obfuscate because i'm lazy

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("RMD | Rate My Avatar", "GrapeTheme")

local Main = Window:NewTab("Main")
local Settings = Window:NewTab("Settings")

-- Main --

local Balls = Main:NewSection("Infinigger")
local Bitches = Main:NewSection("Misc")

Balls:NewButton("AFK", "Permanently AFK Tag", function()
    local Rep = game:GetService("ReplicatedStorage")
    local Star = game.StarterGui
    wait(1.2)
    Star:SetCore("SendNotification", {
    Title = "Instruction";
    Text = "Wait Untill AFK Tag Showed up";
    Icon = "rbxassetid://346007524";
    Duration = 6;
    })
    print("AFK Permanently")
     while wait() do
         Rep.AFK:FireServer()
     end
end)
Balls:NewButton("Sword", "Need to be knight or check Console (F9)", function()
    local Players = game:GetService("Players")
    local Star = game.StarterGui
print("Type 'cs' in  chat to spawn Sword (Knight Required)")
wait(1.2)
Star:SetCore("SendNotification", {
Title = "Instruction";
Text = "Check Console (F9)";
Icon = "rbxassetid://346007524";
Duration = 6;
})
local function chatted(msg)
    if msg == "cs" then
        game:GetService("ReplicatedStorage").RequestTool:FireServer("ClassicSword")
    end
end

Players.LocalPlayer.Chatted:Connect(chatted)
end)
Bitches:NewButton("Infinite Yield", "Commands", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

-- Settings --

local Reload = Settings:NewSection("Surface UI")

Reload:NewKeybind("UI Bind", "Toggling UI", Enum.KeyCode.RightControl, function()
	Library:ToggleUI()
end)
